public class Wife {
    String type;
    public String getType(){
        return type;
    }
    public void setType(String type) {
        this.type = type;
    }
}

public class Who {
    String type;
    public String getType(){
        return type;
    }
    public void setType(String type) {
        this.type = type;
    }
    Who (String type){
        this.type = type;
    }
}

public class Sosed {
    String type;
    public String getType(){
        return type;
    }
    public void setType(String type) {
        this.type = type;
    }
}


